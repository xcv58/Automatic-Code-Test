<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/preference_auth_settings"
        android:key="preference_auth_settings">

        <EditTextPreference android:id="@+id/token"
            android:key="type"
            android:title="@string/preference_auth_type"
            android:summary="@string/preference_auth_type_summary"
            android:defaultValue="@string/preference_auth_type_summary" />

        <EditTextPreference android:id="@+id/token"
            android:key="token"
            android:title="@string/preference_auth_token"
            android:summary="@string/preference_auth_token_summary"
            android:defaultValue="" />
    </PreferenceCategory>
</PreferenceScreen>
